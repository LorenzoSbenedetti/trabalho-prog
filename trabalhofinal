#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define N 5
void bubble_sort (int vetor[], int n) {
    int k, j, aux;

    for (k = 1; k < n; k++) {
        printf("\n[%d] ", k);

        for (j = 0; j < n - 1; j++) {
            printf("%d, ", j);

            if (vetor[j] > vetor[j + 1]) {
                aux          = vetor[j];
                vetor[j]     = vetor[j + 1];
                vetor[j + 1] = aux;
            }
        }
    }
}
void insertionSort(int v[], int n) {
   int i, chave, j;
   for (i = 1; i < n; i++)
   {
       chave = v[i];
       j = i-1;
       while (j >= 0 && v[j] > chave)
       {
           v[j+1] = v[j];
           j = j-1;
       }
       v[j+1] = chave;
   }
}
void selection_sort (int ve[],int max) {
  int i, j, min, aux;

  for (i = 0; i < (max - 1); i++) {
    min = i;
    for (j = i+1; j < max; j++) {
      if (ve[j] < vetor[min]) {
   min = j;
      }
    }
    if (i != min) {
      aux = ve[i];
      ve[i] = ve[min];
      ve[min] = aux;
    }
  }
  for (i = 0; i < max; i++) {
    printf ("%d ",ve[i]);
  }
  printf ("\n");
}
int main(){
    int vet[N],i,op;
    for (i=0;i<N;i++){
            scanf("%d",&vet[i]);
    }
    while (op!=0){
        switch(op){
        case 1:
            bubble_sort(vet[],N);
            break;
        case 2:
            bubble_invertido(vet[],N);
            break;
        case 3:
            bubble_Ponteiros(vet[],N);
            break;
        case 4:
            insertionSort(vet[],N);
            break;
        case 5:
            insertionSort_invertido(vet[],N);
            break;
        case 6:
            insertionSort_ponteiros(vet[],N);
            break;
        case 7:
            selection_sort(vet[],N);
            break;
        case 8:
            selection_sort_invertido(vet[],N);
            break;
        case 9:
            selection_sort_ponteiros(vet[],N);
            break;
        default:
            printf("Nenhum comando escolhido");
    }
   }


